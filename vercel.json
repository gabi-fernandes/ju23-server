{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/getConvidados",
      "dest": "server.js"
    },
    {
      "src": "/adicionar",
      "dest": "server.js"
    }
  ]
}